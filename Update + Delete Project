/* What does the app's SQL look like? */

CREATE table Flo (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT,
    Phase TEXT,
    Cramp_Pain NUMERIC ); 
    
INSERT INTO Flo (name, phase , cramp_Pain)
